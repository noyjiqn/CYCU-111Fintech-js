=IF(A11>=5000, "Sunny", IF(A11 >= 1000, "Cloudy", IF(A11 >= 250, "Bright room", IF(A11 >= 120, "Dimmed room", IF(A11 >= 0, "Dark room")))))
=IF(A13 <=119, "Dark room", IF(A13 <= 249, "Dimmed room", IF(A13 <= 999, "Bright room", IF(A13 <=4999, "Cloudy", IF(A13>= 0, "sunny")))))

